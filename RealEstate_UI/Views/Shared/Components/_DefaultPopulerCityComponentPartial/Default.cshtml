@model List<ResultPopulerCityDto>

<section class="locations-1 popular" id="locations">
    <div class="locations py-5">
        <div class="container py-lg-5 py-md-4">
            <div class="heading text-center mx-auto">
                <h6 class="title-small">Şehirleri Keşfet</h6>
                <h3 class="title-big">Popüler Yerler</h3>
            </div>
            <div class="row pt-5">
                @foreach(var item in Model)
                {
                    <div class="col-lg-3 col-md-4 col-6">
                        <a href="#url">
                            <div class="box16">
                                <img class="img-fluid" src="@item.ImageUrl" alt="">
                                <div class="box-content">
                                    <h3 class="title mb-1">@item.Title</h3>
                                    <span class="post">@item.Count listings</span>
                                </div>
                            </div>
                        </a>
                    </div>
                }
               
               
            </div>
        </div>
    </div>
</section>